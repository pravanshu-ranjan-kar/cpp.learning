#include <iostream>
using namespace std;

int main()
{
int a,b;
cout << "enter two values ";
cin>>a;
cin>>b;
cout<<",the sum of your two values is "<<a+b ;
cout<<",the substraction of your two value is "<<a-b ;
cout<<",the division of your values is "<<a/b ;
cout<<",the multiplication of your value is "<<a*b ;

return 0;
}
